@model SGCFIEE.Models.TablaPafi;
@{
    Layout = "~/Views/Home/Principal.cshtml";
}

<!DOCTYPE html>

<html>
<div class="profile-content">
    <div class="row">
        <div class="col-md-12 col-sm-12">
            <div class="card">
                <div class="card-topline-green"></div>
                <div class="white-box">
                    <div class="p-rl-20">
                        <ul class="nav customtab nav-tabs" role="tablist">
                            <li class="nav-item"><a href="#tab1" class="nav-link active show" data-toggle="tab">@Model.NombrePafi</a></li>
                        </ul>
                    </div>
                    <div class="tab-content">
                        <div class="tab-pane fontawesome-demo active show" id="tab1">
                            <div class="row">
                                <div class="col-md-12 col-sm-12">

                                    <div class="card-body " id="bar-parent2">
                                        <div class="row">
                                            <div class="col-md-6 col-6 b-r">
                                                <label>Profesor</label>
                                                <p id="Matematicas" class="text-muted">@Html.DisplayFor(model => model.NombreMaestro)</p>
                                                <br>
                                            </div>
                                            <div class="col-md-6 col-6 b-r">
                                                <strong>Tipo Pafi</strong>
                                                <p id="Matematicas" class="text-muted">@Html.DisplayFor(model => model.ApePaterno)</p>
                                                <br>
                                            </div>
                                            <div class="col-md-6 col-6 b-r">
                                                <strong>Solicitud</strong>
                                                <p id="Matematicas" class="text-muted">@Html.DisplayFor(model => model.ApePaterno)</p>
                                                <br>
                                            </div>
                                            <div class="col-md-6 col-6 b-r">
                                                <strong>Número de Horas</strong>
                                                <p id="Matematicas" class="text-muted">@Html.DisplayFor(model => model.ApePaterno)</p>
                                                <br>
                                            </div>
                                            <div class="col-md-6 col-6 b-r">
                                                <strong>Periodo</strong>
                                                <p id="Matematicas" class="text-muted">@Html.DisplayFor(model => model.ApePaterno)</p>
                                                <br>
                                            </div>
                                            <div class="col-md-6 col-6 b-r">
                                                <strong>Salon</strong>
                                                <p id="Matematicas" class="text-muted">@Html.DisplayFor(model => model.ClvSalon)</p>
                                                <br>
                                            </div>
                                            <div class="col-md-6 col-6 b-r">
                                                <strong>Número CT</strong>
                                                <p id="Matematicas" class="text-muted">@Html.DisplayFor(model => model.ApePaterno)</p>
                                                <br>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
</html>
